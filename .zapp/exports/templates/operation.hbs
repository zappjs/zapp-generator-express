{{#if imports}}
{{#each imports}}
{{#import this ../allImports '../..'}}
import * as {{{name}}} from '{{{module}}}';
{{/import}}
{{/each}}
{{/if}}

export default async function {{{name}}}({
  {{#each request}}
  {{{@key}}}{{#unless @last}},{{/unless}}
  {{/each}}
}: {
  {{#if request.body}}
  body: {
    {{#each request.body}}
    {{{@key}}}: {{#if type}}{{{type}}}{{else}}any{{/if}};
    {{/each}}
  };
  {{/if}}
  {{#if request.params}}
  params: {
    {{#each request.params}}
    {{{@key}}}: {{{type}}};
    {{/each}}
  };
  {{/if}}
  {{#if request.query}}
  query: {
    {{#each request.query}}
    {{{@key}}}: {{#if type}}{{{type}}}{{else}}any{{/if}};
    {{/each}}
  };
  {{/if}}
}) {
  {{{code handler 1}}}
}
